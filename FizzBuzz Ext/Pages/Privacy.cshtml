@page
@model PrivacyModel
@{
    ViewData["Title"] = "“Ostatnio Szukane";
}
<h1>@ViewData["Title"]</h1>
<br />
@{
    if (Model.FizzBuzzList.Count > 0)
    {
        foreach (Models.FizzBuzz fizz in Model.FizzBuzzList)
        {
            <p>@Html.DisplayNameFor(model => fizz.Number): @fizz.Number </p>
            <p>@Html.DisplayNameFor(model => fizz.Outcome): @fizz.Outcome </p>
            <p>@Html.DisplayNameFor(model => fizz.Date): @fizz.Date </p>
            <br />
        }
    }
    else
    {
        <p>You didn't check any numbers yet</p>
    }
}
